{
  "name": "lighthouse-ui",
  "version": "1.0.0",
  "description": "Lighthouse UI ",
  "type": "module",
  "scripts": {
    "build": "next build",
    "build:dev": "next build --env development",
    "start:dev": "next start --env development",
    "start": "NODE_OPTIONS=--import=./start-me-first.js npm run start:https & next start",
    "local": "GASKET_ENV=local concurrently \"npm run local:https\" \"next dev\"",
    "start:local": "GASKET_DEV=1 node server.js",
    "analyze": "gasket analyze",
    "start:https": "node ./server.js",
    "local:https": "nodemon server.js",
    "upload-coverage": "upload-coverage ",
    "size-limit": "size-limit",
    "lint": "eslint --ext .js,.jsx,.cjs .",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "posttest": "npm run lint && npm run stylelint"
  },
  "dependencies": {
    "@gasket/fetch": "^7.3.6",
    "@gasket/nextjs": "^7.5.4",
    "@gasket/plugin-elastic-apm": "^7.4.7",
    "@gasket/plugin-express": "^7.4.1",
    "@gasket/plugin-https": "^7.3.8",
    "@gasket/plugin-logger": "^7.3.5",
    "@gasket/plugin-webpack": "^7.3.5",
    "@gasket/plugin-winston": "^7.3.5",
    "@gasket/react-intl": "^7.5.3",
    "@godaddy/browserslist-config": "^3.1.0",
    "@godaddy/gasket-auth": "^3.3.10",
    "@godaddy/gasket-header-nav": "^3.4.0",
    "@godaddy/gasket-next": "^3.4.2",
    "@godaddy/gasket-plugin-auth": "^3.3.7",
    "@godaddy/gasket-plugin-dev-certs": "^3.3.6",
    "@godaddy/gasket-plugin-proxy": "^3.3.6",
    "@godaddy/gasket-plugin-security-auth-logging": "^3.3.4",
    "@godaddy/gasket-plugin-security-logger": "^3.3.4",
    "@godaddy/gasket-plugin-self-certs": "^3.3.5",
    "@godaddy/gasket-plugin-uxp": "^3.4.2",
    "@godaddy/gasket-preset-webapp": "^3.4.2",
    "@godaddy/quickcert": "^1.0.1",
    "@ux/alert": "^2400.13.1",
    "@ux/box": "^2400.13.1",
    "@ux/button": "^2400.13.1",
    "@ux/card": "^2400.13.1",
    "@ux/checkbox": "^2400.13.1",
    "@ux/code": "^2400.13.1",
    "@ux/collapsible": "^2400.13.1",
    "@ux/date-input": "^2400.13.1",
    "@ux/dialog": "^2400.13.1",
    "@ux/dropdown": "^2400.13.1",
    "@ux/field-frame": "^2400.13.1",
    "@ux/file-upload": "^2400.13.1",
    "@ux/filter": "^2400.13.1",
    "@ux/flyout": "^2400.13.1",
    "@ux/growl": "^2400.13.1",
    "@ux/icon": "^2400.13.1",
    "@ux/layout": "^2400.13.1",
    "@ux/menu": "^2400.13.1",
    "@ux/message-overlay": "^2400.13.1",
    "@ux/modal": "^2400.13.1",
    "@ux/radio": "^2400.13.1",
    "@ux/search": "^2400.13.1",
    "@ux/select": "^2400.13.1",
    "@ux/select-input": "^2400.13.1",
    "@ux/sibling-set": "^2400.13.1",
    "@ux/space": "^2400.13.1",
    "@ux/spinner": "^2400.13.1",
