{
  "name": "lighthouse-ui",
  "version": "1.0.0",
  "description": "Lighthouse UI ",
  "scripts": {
    "build": "gasket build",
    "start": "gasket start --env development",
    "local": "gasket local",
    "analyze": "gasket analyze",
    "upload-coverage": "upload-coverage ",
    "size-limit": "size-limit",
    "lint": "eslint --ext .js,.jsx,.cjs .",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "posttest": "npm run lint && npm run stylelint"
  },
  "dependencies": {
    "@gasket/assets": "^6.39.0",
    "@gasket/cli": "^6.43.0",
    "@gasket/fetch": "^6.39.0",
    "@gasket/log": "^6.39.0",
    "@gasket/nextjs": "^6.43.0",
    "@gasket/plugin-express": "^6.44.0",
    "@gasket/react-intl": "^6.42.0",
    "@gasket/redux": "^6.39.0",
    "@godaddy/browserslist-config": "^3.0.1",
    "@godaddy/gasket-auth": "^2.15.0",
    "@godaddy/gasket-header-nav": "^2.10.0",
    "@godaddy/gasket-next": "^2.26.3",
    "@godaddy/gasket-plugin-auth": "^2.24.2",
    "@godaddy/gasket-preset-webapp": "^2.27.0",
    "@size-limit/preset-big-lib": "^7.0.5",
    "@ux/alert": "^2301.4.1",
    "@ux/button": "^2301.2.1",
    "@ux/card": "^2301.2.1",
    "@ux/checkbox": "^2301.4.0",
    "@ux/collapsible": "^2301.4.0",
    "@ux/date-input": "^2301.4.0",
    "@ux/filter": "^2301.2.1",
    "@ux/growl": "^2301.4.0",
    "@ux/icon": "^2301.2.1",
    "@ux/layout": "^2301.2.1",
    "@ux/menu": "^2301.2.1",
    "@ux/message-overlay": "^2301.4.2",
    "@ux/modal": "^2301.4.2",
    "@ux/pivot": "^2301.2.1",
    "@ux/search": "^2301.2.1",
    "@ux/select": "^2301.4.0",
    "@ux/select-input": "^2301.2.1",
    "@ux/spinner": "^2301.4.0",
    "@ux/table": "^2301.2.1",
    "@ux/tag": "^2301.4.1",
    "@ux/text-input": "^2301.2.1",
    "@ux/tooltip": "^2301.4.2",
    "@ux/webpack-config": "^2301.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "lodash": "^4.17.21",
    "lodash.merge": "^4.6.0",
    "next": "13.1.1",
    "next-redux-wrapper": "^8.0.0",
    "pm2": "^5.3.1",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^5.25.1",
    "react-redux": "^8.0.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "size-limit": "^7.0.5"
  },
  "devDependencies": {
    "@babel/core": ">=7",
    "@godaddy/eslint-plugin-react-intl": "^1.3.0",
    "@testing-library/react": "^14.0.0",
    "eslint": ">=8",
    "eslint-config-godaddy-react": "^9.0.1",
    "eslint-config-next": "^13.4.19",
    "file-loader": "^6.2.0",
    "postcss": "^8.4.14",
    "sass": "^1.26.10",
    "stylelint": ">=14",
    "stylelint-config-godaddy": "^0.5.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.21.2"
  },
  "browserslist": [
    "extends @godaddy/browserslist-config"
  ],
  "eslintConfig": {
    "extends": [
      "godaddy-react",
      "plugin:@godaddy/react-intl/recommended",
      "next"
    ],
    "settings": {
      "localeFiles": [
        "public/locales/en-US.json"
      ]
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-godaddy"
    ]
  },
  "eslintIgnore": [
    "coverage/",
    "build/"
  ]
}
