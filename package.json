    "amazon-quicksight-embedding-sdk": "^2.10.1",
    "body-parser": "^2.2.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "elastic-apm-node": "^4.13.0",
    "lodash": "^4.17.21",
    "lodash.merge": "^4.6.2",
    "next": "^14.2.30",
    "next-redux-wrapper": "^8.1.0",
    "node-forge": "^1.3.1",
    "papaparse": "^5.5.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^7.1.11",
    "react-redux": "^9.2.0",
    "react-window": "^1.8.11",
